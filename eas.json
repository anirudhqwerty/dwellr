{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "GRADLE_OPTS": "-Dorg.gradle.daemon=false"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "GRADLE_OPTS": "-Dorg.gradle.daemon=false"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "GRADLE_OPTS": "-Dorg.gradle.daemon=false"
      }
    }
  },
  "submit": {
    "production": {}
  }
}